<!DOCTYPE html>
<html lang="en">
  <head>
    <title><%= @view_module.title(@view_template, assigns) %> | Squeeze</title>
    <%= render SqueezeWeb.SharedView, "head.html", assigns %>
    <link rel="stylesheet" href="<%= static_path(@conn, "/css/app.css") %>">
    <script src="<%= static_path(@conn, "/js/app.js") %>"></script>
  </head>

  <body>
    <%= render SqueezeWeb.MenuView, "sidebar.html", assigns %>

    <div class="main-content">
      <%= render SqueezeWeb.MenuView, "topbar.html", assigns %>
      <%= render SqueezeWeb.SharedView, "flash.html", assigns %>
      <%= render @view_module, @view_template, assigns %>
    </div>
  </body>
</html>
