<%= for {_week, dates} <- weeks(assigns) do %>
  <div class="row no-gutters">
    <%= render SqueezeWeb.WeeklySummaryView, "summary.html", Map.put(assigns, :dates, dates) %>

    <%= for date <- dates do %>
      <div class="col-md p-1 calendar-date">
        <div class="pb-1 small">
          <%= date_label(date) %>
        </div>

        <%= render SqueezeWeb.CalendarActivityView, "summary.html", Map.put(assigns, :date, date) %>
      </div>
    <% end %>
  </div>
<% end %>
