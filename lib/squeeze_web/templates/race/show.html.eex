<%= render SqueezeWeb.MenuView, "navbar.html", assigns %>

<main>
  <div class="container mt-6">
    <div class="row">
      <div class="col-md-8">
        <%= render "_hero.html", assigns %>

        <%= if length(@race.trackpoints) > 0 do %>
          <%= render "_course-info.html", assigns %>
        <% end %>

        <div class="mb-6">
          <%= content(assigns) %>
        </div>
      </div>
    </div>
  </div>
</main>
