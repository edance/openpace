<%= @polyline %>
<% data = svg_path(@polyline) %>
<%= if data do %>
<svg id="svg" height="400" width="400" viewport="0 0 400 400" preserveAspectRatio="xMinYMin meet" viewBox="<%= data.x %> <%= data.y %> <%= data.width %> <%= data.height %> ">
  <g>
    <path style="stroke: red; stroke-width: <%= data.stroke_width %>; fill: none;" d="<%= data.path %>"></path>
  </g>
</svg>
<% end %>
